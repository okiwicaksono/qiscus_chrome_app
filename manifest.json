{
  "name": "Qiscus",
  "description": "Secure Chat with your partner",
  "version": "0.1",
  "manifest_version":2,
  "icons":{
    "16":"img/logo16.png",
    "128":"img/logo128.png"
  },
  "browser_action": {
    "default_icon": "img/logo16.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "https://www.qisc.us/"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}